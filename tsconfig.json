{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "migrations/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
